   "source": [
    "# program to create a lambda function that adds 25 to a given number passed in as an argument"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "5d121d5f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter any number: 100\n",
      "125\n"
     ]
    }
   ],
   "source": [
    "def number(x):\n",
    "    y = (lambda x : x+25)(x)\n",
    "    return y\n",
    "a = int(input(\"Enter any number: \"))\n",
    "result = number(a)\n",
    "print(result)"
   ]
